//You have an array of user objects, each one has user.name.
//Write a function that converts it into an array of names.
//For instance
//let mary = {name: "John", age: 25};
//let users = [john, pete, mary]

let john = {name: "John", age: 25};
let peter = {name: "Peter", age: 30};
let users = [john, peter];

function getUserNames(users) {
    return users.map(user => user.name);
}

let names = getUserNames(users);
console.log(names);
