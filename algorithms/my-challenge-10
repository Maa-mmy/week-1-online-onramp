//Create a function unique(arr) that should return 
//an array with unique items of arr.
//Let strings = ["Tosin", "Tobi", "Tosin", "Femi", "Tosin", "Tobi"]

function uniqueItems(arr) {
    let set = new Set(arr);
    let array = new Array(...set);

    return array;
}

console.log(uniqueItems(["Tosin", "Tobi", "Tosin", "Femi", "Tosin", "Tobi"]))